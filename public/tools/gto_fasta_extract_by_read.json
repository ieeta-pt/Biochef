    },
    "output": {
        "type": "stdout",
        "format": "FASTA, Multi-FASTA"
    },
    "flags": [
        {
            "flag": "-h",
            "parameter": null,
            "required": false
        },
        {
            "flag": "-i",
            "parameter": "init",

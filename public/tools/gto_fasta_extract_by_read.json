        },
        {
            "flag": "-e",
            "parameter": "end",
            "required": false
        }
    ],
    "parameters": [
        {
            "name": "init",
            "type": "integer"
        },
        {
            "name": "end",
            "type": "integer"
        }
    ],
    "is_multi_output": false,

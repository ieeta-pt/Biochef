    "input": {
        "type": "stdin",
        "format": "FASTQ"
    },
    "output": {
        "type": "stdout",
        "format": "TEXT"
    },
    "flags": [
        "-h",
        "-v",
        "-a",
        "-s",
        "-i",
        "-p",
        "-k"
    ],
    "parameters": [
        {
            "name": "rFile1",
            "type": "string"
        },
        {
            "name": "rFile2",
            "type": "string"
        },
        {
            "name": "tFile1",
            "type": "string"
        },
        {
            "name": "tFile2",
            "type": "string"
        },
        {
            "name": "bloom size",
            "type": "integer"
        },
        {
            "name": "k-mer size",
            "type": "integer"
        }
    ],
    "is_multi_output": false,
    "is_multi_type_output": false,
    "source": "external_tool (CHESTER)"
}